        "ingredients": [
            "4 chicken breasts",
            "4 slices ham",
            "4 slices Swiss cheese",
            "1 cup breadcrumbs",
            "2 eggs",
            "Salt and pepper"
        ],
        "instructions": [
            "Pound chicken breasts flat and season.",
            "Place ham and cheese on each breast and roll tightly.",
            "Dip in beaten egg and coat with breadcrumbs.",
            "Bake at 400°F (200°C) for 30 minutes or until golden."
        ],
        "cookingTime": "45 minutes",
        "servings": 4,
        "image": "/images/food-images/chicken-cordon.png"

        "ingredients": [
            "2 cups coconut milk",
            "1 cup water",
            "1 sweet potato, diced",
            "1 saba banana, sliced",
            "1 cup cooked tapioca pearls",
            "1/2 cup jackfruit strips",
            "1/4 cup brown sugar",
            "1/4 cup glutinous rice"
        ],
        "instructions": [
            "Boil coconut milk and water in a pot.",
            "Add glutinous rice and cook until soft.",
            "Add sweet potatoes and bananas, cook until tender.",
            "Add sugar, tapioca pearls, and jackfruit.",
            "Simmer until thick and serve warm."
        ],
        "cookingTime": "40 minutes",
        "servings": 4,
        "image": "/images/food-images/binignit.png"
    },
